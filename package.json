{
  "name": "finra-short-api",
  "version": "2.1.5",
  "description": "API for TightShorts.ru - FINRA parser and screener",
  "author": "Sam Gozman <sam@gozman.space>",
  "private": true,
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "export NODE_ENV=production && nest start",
    "start:dev": "export NODE_ENV=development && nest start --watch",
    "start:debug": "export NODE_ENV=development && nest start --debug --watch",
    "start:prod": "export NODE_ENV=production && node dist/main",
    "docker:run:prod": "docker compose -f docker/production/docker-compose.yaml up --build",
    "docker:run:test:e2e": "export TEST_MODE=e2e && docker compose -f docker/testing/docker-compose.yaml up --build",
    "docker:run:test:e2e:watch": "export TEST_MODE=e2e:watch && docker compose -f docker/testing/docker-compose.yaml up --build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "export NODE_ENV=test && jest",
    "test:watch": "jest --clearCache && export NODE_ENV=test && jest --watch --runInBand",
    "test:cov": "export NODE_ENV=test && jest --coverage",
    "test:debug": "export NODE_ENV=test && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "export NODE_ENV=test && jest --config test/jest-e2e.json --detectOpenHandles",
    "test:e2e:watch": "export NODE_ENV=test && jest --config test/jest-e2e.json --detectOpenHandles --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samgozman/finra-short-api.git"
  },
  "bugs": {
    "url": "https://github.com/samgozman/finra-short-api/issues"
  },
  "homepage": "https://github.com/samgozman/finra-short-api#readme",
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.4.2",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.4.2",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^9.2.2",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@sentry/node": "^7.53.1",
    "@sentry/tracing": "^7.53.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "compression": "^1.7.4",
    "date-fns": "^2.30.0",
    "got": "=11.8.5",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "9.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.2.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
