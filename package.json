{
  "name": "finra-short-api",
  "version": "2.1.5",
  "description": "API for TightShorts.ru - FINRA parser and screener",
  "author": "Sam Gozman <sam@gozman.space>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:docker:dev": "docker build . -t ghcr.io/samgozman/finra-short-api:dev ",
    "build:docker:latest": "docker build . -t ghcr.io/samgozman/finra-short-api:latest ",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "export NODE_ENV=production && nest start",
    "start:dev": "export NODE_ENV=development && nest start --watch",
    "start:debug": "export NODE_ENV=development && nest start --debug --watch",
    "start:prod": "export NODE_ENV=production && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "export NODE_ENV=test jest",
    "test:github": "export NODE_ENV=github && jest",
    "test:watch": "jest --clearCache && export NODE_ENV=test && jest --watch --runInBand",
    "test:cov": "export NODE_ENV=test && jest --coverage",
    "test:debug": "export NODE_ENV=test && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "export NODE_ENV=test && jest --config ./test/jest-e2e.json --detectOpenHandles",
    "test:e2e:watch": "export NODE_ENV=test && jest --config ./test/jest-e2e.json --watch --detectOpenHandles",
    "test:e2e:github": "export NODE_ENV=github && jest --config ./test/jest-e2e.json --detectOpenHandles",
    "push:docker:dev": "npm run build:docker:dev && docker push ghcr.io/samgozman/finra-short-api:dev",
    "push:docker:latest": "npm run build:docker:latest && docker push ghcr.io/samgozman/finra-short-api:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samgozman/finra-short-api.git"
  },
  "bugs": {
    "url": "https://github.com/samgozman/finra-short-api/issues"
  },
  "homepage": "https://github.com/samgozman/finra-short-api#readme",
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^8.4.7",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/mongoose": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/terminus": "^8.1.1",
    "@nestjs/throttler": "^4.0.0",
    "@sentry/node": "^7.53.1",
    "@sentry/tracing": "^7.53.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "compression": "^1.7.4",
    "date-fns": "^2.30.0",
    "got": "^12.6.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "9.0.0",
    "mongoose": "^5.13.10",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "=3.0.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^8.4.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^17.0.45",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
