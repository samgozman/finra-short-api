{
    "transform": {
        "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "globalSetup": "<rootDir>/config/jestenv.ts",
    "modulePathIgnorePatterns": ["<rootDir>/__tests__/fixtures"]
}
