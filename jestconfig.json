{
    "transform": {
        "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "setupFiles": ["<rootDir>/config/jestenv.js"],
    "modulePathIgnorePatterns": ["<rootDir>/__tests__/fixtures"]
}
